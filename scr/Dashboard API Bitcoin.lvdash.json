{
  "datasets": [
    {
      "name": "62d26290",
      "displayName": "preco_minimo",
      "queryLines": [
        "SELECT\n",
        "    round(valor_brl,2) AS min_price_brl,\n",
        "    timestamp AS min_timestamp\n",
        "FROM\n",
        "    pipeline_api_bitcoin.bitcoin_data.bitcoin_data\n",
        "WHERE criptomoeda = 'BTC'\n",
        "ORDER BY valor_brl ASC, timestamp DESC\n",
        "LIMIT 1"
      ]
    },
    {
      "name": "bb26fa16",
      "displayName": "preco_maximo",
